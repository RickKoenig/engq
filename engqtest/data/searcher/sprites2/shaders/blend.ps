uniform sampler2D uSampler;
uniform sampler2D uSampler2;
uniform mediump float blend;      
uniform mediump float alphacutoff;      
     
varying highp vec2 vTextureCoord;

void main(void) {
	gl_FragColor = (1.0 - blend)*texture2D(uSampler,vTextureCoord,-1.0) + blend*texture2D(uSampler2,vTextureCoord,-1.0);
//	gl_FragColor.rgb = texture2D(uSampler,vTextureCoord,-1.0).rgb;
//	gl_FragColor.a = texture2D(uSampler2,vTextureCoord,-1.0).a;
	if (gl_FragColor.a < alphacutoff)
		discard;
}
